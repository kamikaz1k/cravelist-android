<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imperialtechnologies.theeatlist_3" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.imperialtechnologies.theeatlist_3.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.imperialtechnologies.theeatlist_3.EditFoodItem"
            android:label="@string/title_activity_edit_food_item"
            android:parentActivityName="com.imperialtechnologies.theeatlist_3.MainActivity" >
        </activity>

        <activity
            android:name="com.imperialtechnologies.theeatlist_3.NewFoodItem"
            android:label="@string/title_activity_new_food_item"
            android:parentActivityName="com.imperialtechnologies.theeatlist_3.MainActivity" >
        </activity>
    </application>

</manifest>
